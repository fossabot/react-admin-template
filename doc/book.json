{
	"gitbook": "3.2.3",
	"title": "PMS项目文档",
	"description": "http://localhost:4000/",
	"author": "sunjingke3@jd.com",
	"language": "zh-hans",
	"plugins": [
		"-lunr",
		"-search",
		"-sharing",
		"include-codeblock",
		"ace",
		"todo",
		"github",
		"splitter",
		"search-pro",
		"mermaid-gb3",
		"hide-element",
		"flexible-alerts",
		"back-to-top-button",
		"expandable-chapters-small"
	],
	"pluginsConfig": {
		"github": {
			"url": ""
		},
		"code": {
			"copyButtons": true
		},
		"include-codeblock": {
			"template": "ace",
			"unindent": true,
			"theme": "monokai"
		},
		"flexible-alerts": {
			"style": "flat"
		},
		"hide-element": {
			"elements": [".gitbook-link"]
		}
	}
}
