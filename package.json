{
  "name": "react-admin-template",
  "version": "0.0.1",
	"main": "build/main/index.js",
	"private": false,
  "description": "React Admin Template",
  "author": {
    "name": "uninge",
    "email": "uninge.sun@gmail.com"
  },
  "scripts": {
    "start": "npm run dev",
    "dev": "cross-env BUILD_ENV=development nodemon .scripts/web/dev.js",
    "electron": "cross-env BUILD_ENV=development node ./.scripts/electron/dev.js",
		"build": "cross-env BUILD_ENV=production node .scripts/web/build.js",
		"package": "cross-env BUILD_ENV=production node ./.scripts/electron/build.js",
    "build:analyze": "cross-env BUNDLE_ANALYZER=1 npm run build",
		"package:analyze": "cross-env BUNDLE_ANALYZER=1 npm run package",
		"pretty-quick": "pretty-quick",
		"prettier": "prettier -c --write src/**/*.{js,jsx,ts,tsx,less,md,json}",
		"fix:styles": "stylelint --fix src/**/*.less --syntax less",
		"fix:scripts": "eslint --fix --ext .js,.jsx,.ts,.tsx ./src",
		"lint-staged": "lint-staged",
    "lint:styles": "stylelint src/**/*.less --syntax less",
    "lint:scripts": "eslint --ext .js,.jsx,.ts,.tsx ./src",
    "lint:prettier": "prettier --check src/**/*.{js,jsx,tsx,ts,less,md,json} --end-of-line auto",
    "cacheclean": "rimraf node_modules/.cache .eslintcache .stylelintcache debug.log yarn-error.log yarn.lock",
		"package:only": "echo \"主线程渲染线程代码已构建，只运行electron-builder打包\" && cross-env PACKAGE_ONLY=1 BUILD_ENV=production node ./.scripts/electron/build.js",
		"svgo": "svgo -f src/assets/svg-sprite --config=./.svgo.yml",
		"postinstall": "electron-builder install-app-deps",
		"dll": "node .scripts/web/dll.js",
		"use": "node .scripts/utils/use.js",
		"prestart": "npm run dll",
    "predev": "npm run use",
    "predll": "npm run use",
    "presvgo": "npm run use",
    "preelectron": "npm run dll",
    "prebuild": "npm run svgo",
    "prepackage": "npm run svgo",
    "test": "cross-env BUILD_ENV=test node .scripts/web/test.js",
    "watch": "tsc --watch",
    "npm-check": "npm-check -u"
  },
