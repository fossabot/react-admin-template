    "webpackbar": "^5.0.0-3"
  },
  "browserslist": {
    "production": [
      "Chrome >= 35",
      "Firefox >= 38",
      "Edge >= 12",
      "not ie < 9",
      "Safari >= 8"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "nodemonConfig": {
    "restartable": "rs",
    "verbose": true,
    "execMap": {
      "js": "node --harmony"
    },
    "env": {
      "NODE_ENV": "development"
    },
    "watch": [
      ".scripts",
      ".antdthemerc.js",
      ".babelrc.js",
      ".eslintrc.js",
      ".stylelintrc.js",
      "dev-proxy.js",
      "package.json",
      "tsconfig.json"
    ],
    "ext": "*.js"
  },
  "repository": {
    "type": "git",
    "url": " https://github.com/uninge/react-admin-template.git"
  },
