{
  "name": "react-admin-template",
  "version": "0.0.1",
  "private": false,
  "description": "React Admin Template",
  "author": {
    "name": "uninge",
    "email": "uninge.sun@gmail.com"
  },
  "scripts": {
    "start": "npm run dev",
    "dev": "cross-env BUILD_ENV=development nodemon ./.scripts/dev.js",
    "build:dev": "cross-env BUILD_ENV=development npm run build",
    "build:test": "cross-env BUILD_ENV=test npm run build",
    "build:prod": "cross-env BUILD_ENV=production npm run build",
    "build:analyze": "cross-env BUNDLE_ANALYZE=true npm run build:prod",
    "lint-staged": "lint-staged",
    "lint:styles": "stylelint \"src/**/*.less\" --syntax less",
    "lint:scripts": "eslint --ext .js,.jsx,.ts,.tsx ./src",
    "fix:styles": "stylelint --fix \"src/**/*.less\" --syntax less",
    "fix:scripts": "eslint --fix --ext .js,.jsx,.ts,.tsx ./src",
    "pretty-quick": "pretty-quick",
    "prettier": "prettier -c --write \"src/**/*.{js,jsx,ts,tsx,less,md,json}\"",
    "lint:prettier": "prettier --check \"src/**/*.{js,jsx,tsx,ts,less,md,json}\" --end-of-line auto",
    "cacheclean": "rimraf node_modules/.cache .eslintcache .stylelintcache debug.log yarn-error.log yarn.lock",
    "watch": "tsc --watch",
    "prestart": "npm run dll",
    "build": "node ./.scripts/pub.js",
    "dll": "node ./.scripts/dll.js",
    "npm-check": "npm-check -u",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "husky": {
    "hooks": {
      "pre-commit": "pretty-quick && npm run lint-staged",
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS",
      "pre-push": "echo \"\n半壁翻云刃，满江酩酊红。千古光阴一霎时，且醉浪涛中！\n\""
    }
  },
  "lint-staged": {
    "src/**/*.less": "npm run lint:styles",
    "src/**/*.{js,jsx,ts,tsx}": "npm run lint:scripts"
  },
